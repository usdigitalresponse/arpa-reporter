{
  "name": "arpa-reporter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm:dev:*\"",
    "dev:vue": "vue-cli-service serve",
    "dev:express": "NODE_ENV=development nodemon src/server",
    "serve:oh": "POSTGRES_URL=postgres://localhost/ohio DATA_DIR=\"./uploads/oh\" yarn serve",
    "serve:ri": "POSTGRES_URL=postgres://localhost/ri DATA_DIR=\"./uploads/ri\"  yarn serve",
    "serve:rp1": "POSTGRES_URL=postgres://localhost/rptest AUDIT=true DATA_DIR=\"./uploads/rp\" yarn serve",
    "serve:rp2": "POSTGRES_URL=postgres://localhost/rp2 AUDIT=true DATA_DIR=\"./uploads/rp\" yarn serve",
    "reset:rptest": "dropdb rptest && createdb --owner=postgres rptest && psql rptest postgres < tests/server-periods/fixtures/rptest.sql",
    "reset:rp1": "dropdb rptest && createdb --owner=postgres rp1 && psql rptest postgres < tests/server-periods/fixtures/rptest.sql",
    "reset:rp2": "dropdb rp2 && createdb --owner=postgres rp2 && psql rp2 postgres < tests/server-periods/fixtures/rp2.sql",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:server": "./tests/server/mocha_wrapper.sh",
    "test:server-period-1": "./tests/server-period-1/mocha_wrapper.sh",
    "test:server-period-2": "./tests/server-period-2/mocha_wrapper.sh",
    "test:server-nodemailer": "./tests/server-nodemailer/mocha_wrapper.sh",
    "test:server-aws-ses": "./tests/server-nodemailer/mocha_wrapper.sh",
    "lint": "vue-cli-service lint",
    "prod": "NODE_ENV=production node src/server",
    "initRender": "./scripts/initRender.mjs"
  },
  "dependencies": {
    "adm-zip": "0.5.9",
    "aws-sdk": "^2.963.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.1",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.21.1",
    "date-fns": "^2.23.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^10.0.1",
    "jquery": "3.6.0",
    "knex": "^1.0.4",
    "lodash": "^4.17.21",
    "lodash-checkit": "^2.4.1",
    "moment": "^2.29.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mustache": "^4.2.0",
    "nodemailer": "^6.6.3",
    "numeral": "^2.0.6",
    "pg": "^8.7.1",
    "popper.js": "1.16.1",
    "uuid": "^8.3.2",
    "vue": "^2.6.13",
    "vue-good-table": "^2.21.11",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "xlsx": "^0.18.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vue/cli": "^4.5.13",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.1",
    "@vue/cli-plugin-router": "~5.0.1",
    "@vue/cli-plugin-unit-mocha": "~5.0.1",
    "@vue/cli-plugin-vuex": "~5.0.1",
    "@vue/cli-service": "~5.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "api": "^4.2.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "chalk": "^5.0.1",
    "concurrently": "7.0.0",
    "cookie-signature": "^1.2.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.5.0",
    "inquirer": "^8.2.2",
    "mocha": "^9.2.1",
    "nodemon": "2.0.15",
    "prettier": "2.6.1",
    "sass": "1.52.1",
    "sass-loader": "13.0.0",
    "standard": "^16.0.3",
    "supertest": "^6.2.3",
    "typescript": "^4.7.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "5.68.0"
  }
}
